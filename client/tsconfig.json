{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "es6"
    ],
    "types": [
      "node"
    ],
    "paths": {
      "@aitheon/billing-manager": [
        "dist/aitheon/billing-manager"
      ],
      "@aitheon/billing-manager/*": [
        "dist/aitheon/billing-manager/*"
      ],
      "@aitheon/billing-client": [
        "dist/aitheon/billing-client"
      ],
      "@aitheon/billing-client/*": [
        "dist/aitheon/billing-client/*"
      ]
    }
  }
}
